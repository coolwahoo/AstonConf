    "version": "0.2.0",
    "configurations": [
        {
            "type": "edge",
            "request": "launch",
            "name": "Launch Edge",
            "url": "http://localhost:5501/index.htm",
            "webRoot": "${workspaceFolder}/wwwroot"
        },

        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch schedule.htm",
            "url": "http://localhost:5501/schedule.htm",
            "webRoot": "${workspaceFolder}/schedule.htm"
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}